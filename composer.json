{
  "name": "zanphp/http-client",
  "require": {
    "php": ">=5.6",
    "zanphp/dns-client": "dev-master",
    "zanphp/exception": "dev-master",
    "zanphp/coroutine": "dev-master",
    "zanphp/timer": "dev-master",
    "zanphp/rpc-context": "dev-master"
  },
  "require-dev": {
    "zanphp/testing":"dev-master"
  },
  "autoload": {
    "psr-4": {
      "ZanPHP\\HttpClient\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ZanPHP\\HttpClient\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "preferred-install": "dist",
    "secure-http": false
  },
  "minimum-stability": "dev"
}